	     x:Class="Tableau.Migration.App.GUI.App"
	     xmlns:local="using:Tableau.Migration.App.GUI"
	     x:DataType="local:App"
	     RequestedThemeVariant="Light">
    <NativeMenu.Menu>
	<NativeMenu>
	    <NativeMenuItem Header="{Binding AppNameVersion}">
	    </NativeMenuItem>
	</NativeMenu>
    </NativeMenu.Menu>
    <Application.DataTemplates>
