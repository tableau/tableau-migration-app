{
  "metadata": [
    {
      "src": [
	{
	  "src": "../src",
	  "files": [
	    "**/*.csproj"
	  ],
	}
      ],
      "dest": "api",
      "disableGitFeatures": true
    }
  ],
  "build": {
    "content": [
      {
	"files": [
	  "**/*.{md,yml}"
	],
	"exclude": [
	  "_site/**"
	]
      },
      {
	"files": [
	  "readme.md"
	],
	"src": "../",
	"dest": "index.md"
      }
    ],
    "resource": [
      {
	"files": [
	  "images/**",
	  "screenshots/**"
	]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appName": "Tableau Migration App",
      "_appTitle": "Tableau Migration App",
      "_enableSearch": true,
      "_disableContribution": true,
      "_gitContribute": {
	"branch": "",
	"path": ""
      },
      "gitUrlPattern": "github",
      "pdf": true
    }
  }
}
